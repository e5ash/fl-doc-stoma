.price-list.tabs
	.case
		if priceListTitle != undefined
			.price-list__title.h2.tac!= priceListTitle
		.price-list__wrap
			if priceListShow != false
				.price-list__nav.tabs-nav
					ul
						li
							a(href="#price-list-item-all") Все услуги
						li
							a(href="#price-list-item-1") Диагностика и консультация  
						li
							a(href="#price-list-item-2") Отбеливание
						li
							a(href="#price-list-item-3") Чистка
						li
							a(href="#price-list-item-4") Имплантация
						li
							a(href="#price-list-item-5") Ортопедия
						li
							a(href="#price-list-item-6") Хирургия
						li
							a(href="#price-list-item-7") Костная пластика
						li
							a(href="#price-list-item-8") Пародонтология
						li
							a(href="#price-list-item-9") Ортодонтия
						li
							a(href="#price-list-item-10") Детская стоматология
				+swipe('price-list__nav-swipe', 'Проведите в сторону, чтобы <br>посмотреть все услуги')
			.price-list__list.tabs-list#price-list
				- var i = 1
				- var parsePrice
				if price != undefined
					- var parsePrice = price
				else
					- var parsePrice = _prices
				each priceItem in parsePrice
					+price-item(i, priceItem[0], _prices[0][1])
					- i++


	
